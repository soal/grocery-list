{
	"files": ["src/interop.ts"],
	"compilerOptions": {
		"target": "es2019",
		"module": "esnext",
		"lib": ["esnext", "dom"],
		"noEmit": true,
		"strict": false,
		"importHelpers": true,
		"resolveJsonModule": true,
